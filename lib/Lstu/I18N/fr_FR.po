msgid "My links"
msgstr "Mes liens"

#: templates/index.html.ep:95
msgid "Next step: share your short link with whoever you wish."
msgstr "Partagez ensuite avec vos correspondants le lien qui vous est donné."

#: templates/index.html.ep:93
msgid "Paste a long URL to make it shorter."
msgstr "Collez le lien à raccourcir dans le formulaire."

#: templates/index.html.ep:7
msgid "Shorten a link"
msgstr "Raccourcir un lien"

#: templates/layouts/default.html.ep:20
msgid "Shorten your URLs"
msgstr "Raccourcissez vos URL"

#: templates/index.html.ep:127
msgid "Support the author on Liberapay"
msgstr "Supporter l’auteur sur Liberapay"

#: templates/index.html.ep:123
msgid "The software"
msgstr "Le logiciel"

#: templates/index.html.ep:105
msgid "Video tutorial"
msgstr "Tutoriel vidéo"

#: templates/index.html.ep:112
msgid ""
"Watch <a href=\"https://old.framatube.org/blip/framalink.webm\">the webm "
"video</a>"
msgstr ""
