msgstr "I miei links"

#: templates/index.html.ep:95
msgid "Next step: share your short link with whoever you wish."
msgstr "Prossimo step: condividi il tuo link accorciato con chi vuoi."

#: templates/index.html.ep:93
msgid "Paste a long URL to make it shorter."
msgstr "Incolla l'URL lungo per crearne uno abbreviato."

#: templates/index.html.ep:7
msgid "Shorten a link"
msgstr "Abbreviare un link"

#: templates/layouts/default.html.ep:20
msgid "Shorten your URLs"
msgstr "Abbrevia i tuoi URL"

#: templates/index.html.ep:127
msgid "Support the author on Liberapay"
msgstr "Sostieni l'autore con Liberapay"

#: templates/index.html.ep:123
msgid "The software"
msgstr "Il programma"

#: templates/index.html.ep:105
msgid "Video tutorial"
msgstr "Video tutorial"

